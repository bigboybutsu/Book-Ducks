{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "pages": {
      "type": "integer"
    },
    "coverImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "releaseDate": {
      "type": "date"
    },
    "rating": {
      "type": "json"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "books"
    }
  }
}
